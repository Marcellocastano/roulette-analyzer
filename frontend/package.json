{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc -b && vite build --mode production",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:prod": "vite preview --mode production",
    "lint": "eslint --ext .js,.vue,.ts src/",
    "lint:fix": "eslint --ext .js,.vue,.ts src/ --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,scss,css}\""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "vue-i18n": "^9.14.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vicons/tabler": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.5.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "naive-ui": "^2.41.0",
    "postcss": "^8.4.21",
    "prettier": "^3.2.5",
    "sass": "^1.84.0",
    "tailwindcss": "^3.3.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.7"
  }
}
